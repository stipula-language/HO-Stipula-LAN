stipula Deposit {
  asset flour
  field cost_flour
  init Start

  agreement (Client, Farm)(cost_flour) {
  Client, Farm : cost_flour

  } ==> @Start

@Start Farm : begin()[]{
  cost_flour -> Client;
  now + 365 >> @Run { cost_flour -> Client } ==> @End
} ==> @Run

@Run Farm : send()[h]{
  h -> Client
  h -o flour;
_
} ==> @Run

@Run Farm : buy()[](w<=(flour*cost_flour)){
  ((10/flour) * cost_flour) -o flour, Client;
_
} ==> @Run

}